
@{
    ViewBag.Title = "VissionMission";
    Layout = "~/Views/Shared/WebsiteLayout.cshtml";
}






<div id="carousel" class="carousel slide" data-ride="carousel">

    <div class="carousel-inner" role="listbox">

        <div class="carousel-item active" style="background-image: url('../AdminCss/assets/img/vm-banner.png'); background-size: cover; ">
